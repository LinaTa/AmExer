<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.example.livruen.amexer.Maps"
    android:background="#ffffff">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText_maps"
        android:background="@color/wolfenbuettelBlue"
        android:text="Campus am Exer"
        android:focusable="false"
        android:textColor="#ffffff"
        android:layout_row="0"
        android:layout_column="0"
        android:inputType="none"
        android:layout_columnSpan="2"
        android:gravity="center"
        android:layout_gravity="left|top"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/map_exer"
        android:layout_below="@+id/editText_maps"
        android:layout_alignParentStart="true"
        android:contentDescription="ExerMap"
        android:layout_alignRight="@+id/gebaeude23"
        android:layout_alignEnd="@+id/gebaeude23" />

    <Button
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginLeft="130dp"
        android:layout_marginStart="130dp"
        android:id="@+id/gebaeude8"
        android:layout_below="@+id/editText_maps"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="80px"
        android:layout_height="80px"
        android:id="@+id/gebaeude23"
        android:clickable="true"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/gebaeude8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="80px"
        android:layout_height="200px"
        android:layout_marginRight="38dp"
        android:layout_marginEnd="38dp"
        android:id="@+id/gebaeude6"
        android:clickable="true"
        android:background="@android:color/transparent"
        android:layout_toLeftOf="@+id/gebaeude12"
        android:layout_toStartOf="@+id/gebaeude12" />

    <Button
        android:layout_width="60px"
        android:layout_height="20px"
        android:layout_marginRight="66dp"
        android:layout_marginEnd="66dp"
        android:layout_alignTop="@+id/gebaeude6"
        android:layout_alignRight="@+id/gebaeude23"
        android:layout_alignEnd="@+id/gebaeude23"
        android:layout_alignBottom="@+id/gebaeude6"
        android:id="@+id/gebaeude10"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="80px"
        android:layout_height="200px"
        android:layout_marginTop="17dp"
        android:id="@+id/gebaeude4"
        android:layout_below="@+id/gebaeude6"
        android:layout_alignLeft="@+id/gebaeude6"
        android:layout_alignStart="@+id/gebaeude6"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="80px"
        android:layout_height="200px"
        android:id="@+id/gebaeude12"
        android:layout_alignTop="@+id/gebaeude4"
        android:layout_alignRight="@+id/gebaeude10"
        android:layout_alignEnd="@+id/gebaeude10"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="80px"
        android:layout_height="100px"
        android:id="@+id/gebaeude2"
        android:layout_below="@+id/gebaeude4"
        android:layout_alignLeft="@+id/gebaeude8"
        android:layout_alignStart="@+id/gebaeude8"
        android:layout_marginTop="17dp"
        android:layout_alignRight="@+id/gebaeude8"
        android:layout_alignEnd="@+id/gebaeude8"
        android:clickable="true"
        android:elegantTextHeight="false"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="20dp"
        android:id="@+id/gebaeude11"
        android:clickable="true"
        android:background="@android:color/transparent"
        android:layout_marginBottom="12dp"
        android:layout_alignBottom="@+id/gebaeude4"
        android:layout_toLeftOf="@+id/gebaeude4"
        android:layout_toStartOf="@+id/gebaeude4"
        android:layout_marginRight="22dp"
        android:layout_marginEnd="22dp" />

    <Button
        android:layout_width="45dp"
        android:layout_height="20dp"
        android:id="@+id/gebaeude7"
        android:layout_alignBottom="@+id/gebaeude2"
        android:layout_alignLeft="@+id/gebaeude11"
        android:layout_alignStart="@+id/gebaeude11"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="40dp"
        android:id="@+id/gebaeude5"
        android:layout_below="@+id/gebaeude7"
        android:layout_alignRight="@+id/gebaeude7"
        android:layout_alignEnd="@+id/gebaeude7"
        android:layout_marginTop="17dp"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="20dp"
        android:layout_height="40dp"
        android:id="@+id/gebaeude3"
        android:layout_alignTop="@+id/gebaeude5"
        android:layout_toRightOf="@+id/gebaeude11"
        android:layout_toEndOf="@+id/gebaeude11"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:id="@+id/gebaeude45"
        android:layout_alignTop="@+id/gebaeude1"
        android:layout_alignLeft="@+id/gebaeude12"
        android:layout_alignStart="@+id/gebaeude12"
        android:layout_marginTop="8dp"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:id="@+id/gebaeude1"
        android:layout_alignBottom="@+id/gebaeude3"
        android:layout_toRightOf="@+id/gebaeude3"
        android:layout_toEndOf="@+id/gebaeude3"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:clickable="true"
        android:background="@android:color/transparent"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="20dp"
        android:id="@+id/gebaeude9"
        android:layout_above="@+id/gebaeude7"
        android:layout_alignRight="@+id/gebaeude7"
        android:layout_alignEnd="@+id/gebaeude7"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp"
        android:layout_below="@+id/gebaeude4"
        android:clickable="true"
        android:onClick="setActions"
        android:background="@android:color/transparent"/>

</RelativeLayout>
