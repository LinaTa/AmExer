<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context=".Library"
    android:background="@color/backgroundOstfalia">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:background="@color/backgroundOstfalia"
        android:id="@+id/frameLayout"
        android:layout_marginTop="25dp">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="243dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginTop="30dp">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:baselineAligned="false">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView"
                    android:layout_column="0"
                    android:gravity="center"
                    android:background="@drawable/cell_shape"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Semester"
                    android:id="@+id/textView2"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Vorlesungsfrei"
                    android:id="@+id/textView3"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:width="100dp"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="   Mo   "
                    android:id="@+id/textView4"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          09:30 - 18:00          "
                    android:id="@+id/Mo_Se"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          09:30 - 15:00          "
                    android:id="@+id/Mo_Vf"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Di"
                    android:id="@+id/textView7"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" 09:30 - 18:00 "
                    android:id="@+id/Tue_Se"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="09:30 - 18:00"
                    android:id="@+id/Tue_Vf"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mi"
                    android:id="@+id/textView10"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="09:30 - 18:00"
                    android:id="@+id/We_Se"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="09:30 - 15:00"
                    android:id="@+id/We_Vf"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Do"
                    android:id="@+id/textView13"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="09:30 - 18:00"
                    android:id="@+id/Thu_Se"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="09:30 - 15:00"
                    android:id="@+id/Thu_Vf"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fr"
                    android:id="@+id/textView14"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="09:30 - 15:00"
                    android:id="@+id/Fr_Se"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="09:30 - 13:00"
                    android:id="@+id/Fr_Vf"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

        </TableLayout>

        <TextView
            android:layout_width="350dp"
            android:layout_height="149dp"
            android:text="Open"
            android:id="@+id/library_text"
            android:layout_column="3"
            android:layout_gravity="center_horizontal|top"
            android:gravity="center"
            android:background="@drawable/cell_shape"
            android:textColor="#3ed311"
            android:textSize="30dp"
            android:textIsSelectable="false" />

    </FrameLayout>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText_library"
        android:background="@color/wolfenbuettelBlue"
        android:text="@string/library"
        android:focusable="false"
        android:textColor="#ffffff"
        android:layout_row="0"
        android:layout_column="0"
        android:inputType="none"
        android:layout_columnSpan="2"
        android:gravity="center"
        android:layout_gravity="left|top"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
