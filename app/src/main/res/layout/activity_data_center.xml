<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context=".DataCenter">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="30dp">

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:id="@+id/library_text"
            android:layout_column="3"
            android:layout_gravity="center_horizontal|top"
            android:gravity="center"
            android:background="@drawable/cell_shape"
            android:textColor="@color/ostfaliaGreen"
            android:textSize="30dp"
            android:textIsSelectable="false"
            android:layout_weight="0.9" />

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="0.5">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:baselineAligned="false">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView"
                    android:layout_column="0"
                    android:gravity="center"
                    android:background="@drawable/cell_shape"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Semester"
                    android:id="@+id/textView2"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp"
                    android:layout_span="3" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="   Mo   "
                    android:id="@+id/textView4"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          09:00 - 12:00          "
                    android:id="@+id/Mo_1"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          13:00 - 15:30          "
                    android:id="@+id/Mo_2"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Di"
                    android:id="@+id/textView7"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          09:00 - 12:00          "
                    android:id="@+id/Tue_1"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          13:00 - 15:30          "
                    android:id="@+id/Tue_2"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mi"
                    android:id="@+id/textView10"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          09:00 - 12:00          "
                    android:id="@+id/We_1"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          13:00 - 15:30          "
                    android:id="@+id/We_2"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Do"
                    android:id="@+id/textView13"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          09:00 - 12:00          "
                    android:id="@+id/Thu_1"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          13:00 - 15:30          "
                    android:id="@+id/Thu_2"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fr"
                    android:id="@+id/textView14"
                    android:layout_column="0"
                    android:background="@drawable/cell_shape"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="          09:00 - 12:00          "
                    android:id="@+id/Fr_1"
                    android:layout_column="1"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/Fr_2"
                    android:layout_column="3"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:height="30dp" />
            </TableRow>

        </TableLayout>

    </LinearLayout>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/editText_library"
        android:background="@color/wolfenbuettelBlue"
        android:text="Rechenzentrum"
        android:focusable="false"
        android:textColor="#ffffff"
        android:layout_row="0"
        android:layout_column="0"
        android:inputType="none"
        android:layout_columnSpan="2"
        android:gravity="center"
        android:layout_gravity="left|top"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
